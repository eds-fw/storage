  "main": "index",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eds-fw/storage.git"
  },
  "keywords": [
    "typescript",
    "javascript",
    "storage",
    "json-database",
    "key-value storage"
  ],
  "author": "dtkdtk0",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/eds-fw/storage/issues"
  },
