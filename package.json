{
  "devDependencies": {
    "@types/node": "^20.12.13",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "TS_inspect": "npx tsc --noEmit --watch",
    "TS_build": "npx tsc -p tsconfig.cjs.json && npx tsc -p tsconfig.esm.json && npx tsc -p tsconfig.types.json"
  },
  "dependencies": {
    "@eds-fw/utils": "^2.0.0",
    "array-equal": "^1.0.0",
    "fast-deep-equal": "^3.1.3",
    "tslib": "^2.6.1"
  },
  "name": "@eds-fw/storage",
